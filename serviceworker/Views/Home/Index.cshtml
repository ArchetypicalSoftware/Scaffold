@{
    ViewData["Title"] = "Home Page";
}

<input type="text" id="JsonId" />
<input type="button" id="JsonButton" value="Click me" />

<script>
    document.getElementById('JsonButton').addEventListener("click", send);

    function send() {
        var value = document.getElementById('JsonId').value;
        fetch("/Home/JsonData/?id=" + value).then(function (response) {
            return response.json();
        }).then(function (json) {
            console.log(JSON.stringify(json));
        });
    }

    fetch("Home/StaticJsonData");
</script>
