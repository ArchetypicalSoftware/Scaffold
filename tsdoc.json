{
	"tags":
	{
		"allowUnknownTags" : true
	},
	"plugins":[
		 "plugins/markdown"
		,"/code/scaffold/src/scaffold/node_modules/tsdoc/template/plugins/TSDoc.js"
	],
	"opts":
	{
		"template"	:"/code/scaffold/src/scaffold/node_modules/tsdoc/template",
		"recurse"	:"true"
	},
	"templates" : {
		"cleverLinks"     : false,
		"monospaceLinks"  : false
	},
	"source":
	{
		"includePattern": "(\\.d)?\\.ts$"
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
	"tsdoc":{
		"source"		:"/code/scaffold/src/scaffold/src/",
		"destination"	:"/code/scaffold/src/scaffold/docs",
		"tutorials"		:"",
		"systemName"	: "scaffold",
		"footer"		: "",
		"copyright"		: "Archetypical Software Copyright Â© 2019",
		"outputSourceFiles" : true,
		"commentsOnly": true
	}
}
